{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "types": [
      "vite/client"
    ],
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.d.ts",
    "./**/*.vue"
  ]
}